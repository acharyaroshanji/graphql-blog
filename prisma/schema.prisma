generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model posts {
  author Int?
  body   String
  id     Int    @default(autoincrement()) @id
  title  String
  user   users? @relation(fields: [author], references: [id])
}

model users {
  age   Int
  email String  @unique
  id    Int     @default(autoincrement()) @id
  name  String
  posts posts[]
}